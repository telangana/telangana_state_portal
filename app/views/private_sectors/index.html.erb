<h1>Listing private_sectors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Website</th>
    <th>Facebook page</th>
    <th>District</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @private_sectors.each do |private_sector| %>
  <tr>
    <td><%= private_sector.name %></td>
    <td><%= private_sector.description %></td>
    <td><%= private_sector.website %></td>
    <td><%= private_sector.facebook_page %></td>
    <td><%= private_sector.district_id %></td>
    <td><%= link_to 'Show', private_sector %></td>
    
  </tr>
<% end %>
</table>

